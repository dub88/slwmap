{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "routes": [
    { "src": "/_next/static/(.*)", "dest": "/_next/static/$1" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/404", "status": 404 },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
